#!/usr/bin/env python
class BubbleSort(object):
    def sort(self, arr):
        total = len(arr)
        for i in range(total):
            for j in range(i + 1, total):
                if(arr[i] > arr[j]):
                    tmp = arr[j]
                    arr[j] = arr[i]
                    arr[i] = tmp
        return arr

print BubbleSort().sort([1, 3, 4, 55, 22, 8, 96, 0])
